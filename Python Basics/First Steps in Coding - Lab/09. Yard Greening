square_meters = float(input())
price_per_square_meter = 7.61
discount = 0.18 # 18 percent
final_price = square_meters * price_per_square_meter * (1-discount)
discounted_price = square_meters * price_per_square_meter * discount

print(f"The final price is: {final_price} lv.")
print(f"The discount is: {discounted_price} lv.")
