pen_pack_price = 5.80
marker_pack_price = 7.20
cleaning_detergent_price = 1.20

pen_packs = int(input())
marker_packs = int(input())
cleaning_detergent = int(input())
discount = (1 - int(input()) / 100)

print((pen_packs * pen_pack_price + marker_packs * marker_pack_price + cleaning_detergent * cleaning_detergent_price) * discount)
